{
    "extends": "expo/tsconfig.base",
    "compilerOptions": {
        "strict": true,
        "jsx": "react-native",
        "lib": [
            "esnext"
        ],
        "moduleResolution": "node",
        "skipLibCheck": true,
        "noEmit": true
    },
    "include": ["**/*.ts", "**/*.tsx", ".expo/types/**/*.ts", "app.d.ts", "expo-env.d.ts", "app/(tabs)/index.tsx"]
}
